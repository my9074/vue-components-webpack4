{
  "name": "sk-element-webpack",
  "version": "0.0.1-alpha.11",
  "description": "",
  "main": "./dist/sk-element.min.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "",
    "build:dist": "webpack --config ./build/webpack.dist.conf.js",
    "build:components": "webpack --config ./build/webpack.dist.component.conf.js",
    "build:utils": "cross-env BABEL_ENV=utils babel src --out-dir dist --ignore src/*.js",
    "build:theme": "gulp build --gulpfile packages/theme-default/gulpfile.js && cp-cli packages/theme-default/dist dist/theme-default",
    "clean": "rimraf dist",
    "dist": "npm run clean && npm run build:utils && npm run build:dist && npm run build:components && npm run build:theme",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:public": "npm run docs:build && gh-pages -d docs/.vuepress/dist --remote origin && rimraf docs/.vuepress/dist",
    "lint": "eslint src/**/* packages/**/*.{js,vue} build/**/* --quiet",
    "commit": "git-cz"
  },
  "author": "",
  "license": "ISC",
  "files": [
    "dist/*",
    "src/*",
    "*.json",
    "*.js"
  ],
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-object-assign": "^7.0.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "cp-cli": "^1.1.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.7.0",
    "eslint-plugin-vue": "^5.0.0-beta.3",
    "gh-pages": "^2.0.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-less": "^4.0.1",
    "less-loader": "^4.1.0",
    "rimraf": "^2.6.2",
    "url-loader": "^1.1.2",
    "vue": "^2.5.17",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "vuepress": "^0.14.4",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "deepmerge": "^2.2.1"
  },
  "peerDependencies": {
    "vue": "^2.3.0",
    "axios": "^0.18.0"
  }
}
